{
  "expo": {
    "name": "Blood Rhapsody",
    "slug": "vampire-rhythm-game",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./client/public/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./client/public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.vamprhythm.game"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./client/public/icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.vamprhythm.game",
      "permissions": [
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "favicon": "./client/public/favicon.ico"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "70ac59fa-1662-46af-b2cd-f6fbbef45298"
      }
    }
  }
}
